plugin: openapi
specFile: openapi.yml

resources:
  - path: "/start?personType=donor&lpas[]=M-XYXY-YAGA-35G3"
    method: get
    response:
      statusCode: 200
      scriptFile: useDonorJsonFile.groovy

  - path: "/start?personType=certificateProvider&lpas[]=M-XYXY-YAGA-35G3"
    method: get
    response:
      statusCode: 200
      scriptFile: useCertificateProviderJsonFile.groovy
